
import java.util.*;

class Solution {
    public static void main(String[] args) {
        String str ="arunpraveenrajs";
        removeVowels(str);
        
    }
    public static void removeVowels(String str){
        for(int i =0;i<str.length();i++){
            char ch = str.charAt(i);
            if(ch == 'a'||ch =='e'||ch =='i'||ch =='o'||ch =='u'){
            continue;
        }
        
        System.out.print(ch);
        }
    }
}
